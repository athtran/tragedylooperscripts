<p id="notice"><%= notice %></p>

<h1>Listing Tsets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tsets.each do |tset| %>
      <tr>
        <td><%= tset.title %></td>
        <td><%= link_to 'Show', tset %></td>
        <td><%= link_to 'Edit', edit_tset_path(tset) %></td>
        <td><%= link_to 'Destroy', tset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tset', new_tset_path %>
